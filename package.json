{
  "name": "slidev-addon-java-runner",
  "version": "0.1.0",
  "description": "Slidev addon for Java code runner.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "slidev --remote --bind 127.0.0.1 example.md",
