    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ObjectOops/slidev-addon-java-runner.git"
  },
  "keywords": [
    "slidev-addon",
    "slidev",
    "java",
    "cheerp",
    "cheerpj"
  ],
  "author": "ObjectOops",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ObjectOops/slidev-addon-java-runner/issues"
  },
  "homepage": "https://github.com/ObjectOops/slidev-addon-java-runner#readme",
  "files": [
    "bootstrap",
    "setup",
    "example.md",
    "CHANGELOG.md"
  ],
  "devDependencies": {
    "@slidev/cli": "^51.8.2",
    "@slidev/theme-default": "^0.25.0"
  }
}
